/dts-v1/;
/plugin/;

/ {
	compatible = "ti,beaglebone", "ti,beaglebone-black";

	/* identification */
	part-number = "bb_motor_cape";
        version = "00A0";

	fragment@0 {
		target = <&am33xx_pinmux>;
		__overlay__ {
			pinctrl_test: bb_motor_cape_pins {
				pinctrl-single,pins = <
                                        0x090 0x07	/* P8_7 muxRegOffset, OUTPUT | MODE7 */
                                        //0x080 0x07	/* P8_21 muxRegOffset, OUTPUT | MODE7 */
					//0x010 0x07	/* P8_23 muxRegOffset, OUTPUT | MODE7 */
					//0x000 0x07	/* P8_25 muxRegOffset, OUTPUT | MODE7 */
					//0x0e0 0x07	/* P8_27 muxRegOffset, OUTPUT | MODE7 */
					//0x0e4 0x07	/* P8_29 muxRegOffset, OUTPUT | MODE7 */
					//0x0d8 0x07	/* P8_31 muxRegOffset, OUTPUT | MODE7 */
					//0x0d4 0x07	/* P8_33 muxRegOffset, OUTPUT | MODE7 */
					//0x0d0 0x07	/* P8_35 muxRegOffset, OUTPUT | MODE7 */
					//0x018 0x07	/* P8_3 muxRegOffset, OUTPUT | MODE7 */
					//0x008 0x07	/* P8_5 muxRegOffset, OUTPUT | MODE7 */
				>;
			};
		};
	};

	fragment@1 {
		target = <&ocp>;
		__overlay__ {
			test_helper: helper {
				compatible = "bone-pinmux-helper";
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_test>;
				status = "okay";
			};
		};
	};
};
